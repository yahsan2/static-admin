{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd .. && pnpm install && pnpm build",
  "outputDirectory": "dist",
  "installCommand": "echo 'skip'",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/[[...route]]"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
