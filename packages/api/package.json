{
  "name": "@static-admin/api",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@noble/hashes": "^1.7.1",
    "@static-admin/core": "workspace:*",
    "better-sqlite3": "^11.7.0",
    "nodemailer": "^7.0.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/nodemailer": "^7.0.4",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@static-admin/core": "workspace:*"
  }
}
