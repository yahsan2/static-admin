{
  "name": "@static-admin/hono",
  "version": "0.1.0",
  "description": "Hono adapter with auth middleware for Static Admin CMS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahsan2/static-admin.git",
    "directory": "packages/hono"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@static-admin/api": "workspace:*",
    "@static-admin/cms": "workspace:*",
    "@static-admin/core": "workspace:*"
  },
  "devDependencies": {
    "hono": "^4.6.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  }
}
